.loginlist{ width:350px; margin:190px auto; border:1px solid #f6f6f6; font-size:14px; color:#767676;}
.loginlist td{ padding:10px;text-align:center;}
.loginlist tr.loginlist_title{ background:#fffae5; border-bottom:1px solid #fbf3d3;}
.loginlist tr{ background:rgba(255,255,255,0.3)}
